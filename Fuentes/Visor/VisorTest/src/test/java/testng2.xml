<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="VisorTestSuite" parallel="false">
	<test name="CategoriaTest" enabled="false">
		<parameter name="nombreInsertar" value="Chocolates" />
		<parameter name="nombreActualizar" value="Lacteos" />
		<classes>
			<class name="pe.com.test.CategoriaTest" />
		</classes>
	</test>
	<test name="CategoriaWebDriverTest">
		<parameter name="navegador" value="firefox"/>
		<parameter name="remoto" value="0"/>
		<classes>
			<class name="pe.com.test.seleniumwd.CategoriaWebDriverTest" />
		</classes>
	</test>
	<test name="ProductoTest" enabled="false">
		<parameter name="nombreInsertar" value="Chocolates" />
		<parameter name="nombreActualizar" value="Lacteos" />
		<classes>
			<class name="pe.com.test.ProductoTest" />
		</classes>
	</test>
	<test name="ProductoWebDriverTest">
		<parameter name="navegador" value="firefox"/>
		<parameter name="remoto" value="0"/>
		<classes>
			<class name="pe.com.test.seleniumwd.ProductoWebDriverTest" />
		</classes>
	</test>
</suite> 